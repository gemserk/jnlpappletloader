<?xml version="1.0" encoding="utf-8"?>
	<jnlp spec="$jnlpspec" codebase="$webstartCodebase" href="launch.jnlp">
  	<information>
    <title>$webstartName</title>
    <vendor>$project.Organization.Name</vendor>
    <homepage href="$project.Url"/>
    <description>$project.Description</description>
#if($offlineAllowed)
    <offline-allowed/>
#end
  </information>
#if($allPermissions)
  <security>
     <all-permissions/>
  </security>
#end
	<resources>
	    <j2se version="$j2seVersion"/>
	     #foreach( $dependency in $fulldependencies )
			#if ( !$dependency.classifier)
				<jar href="${dependency.artifactId}-${dependency.version}.jar" />
			#end
		#end
	    #foreach( $dependency in $fulldependencies )
			#if ( $dependency.classifier && !$dependency.classifier.startsWith('natives-'))
				<jar href="${dependency.artifactId}-${dependency.version}-${dependency.classifier}.jar" />
			#end
		#end
	</resources>
  
    <applet-desc name="$project.Description" main-class="$mainClass" width="640" height="480" />
  	
</jnlp>
